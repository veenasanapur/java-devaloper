package Day9;
class Student
{
	String name;
	int marks;
}
public class StudentDetails {

	public static void main(String[] args) {
		Student s1 =new Student();
		Student s2= new Student();
		System.out.println(s1.name+" "+s1.marks);
		System.out.println(s2.name+" "+s2.marks);
	}

}
output:
null 0
null 0
